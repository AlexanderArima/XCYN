
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>登陆</title>
</head>
<body>
    <div> 
        <form action="Login" method="post">
            <table>
                <tr>
                    <td>用户名：</td>
                    <td><input type="text" name="UserName" id="TextUserName" /></td>
                </tr>
                <tr>
                    <td>密码：</td>
                    <td><input type="password" name="Password" id="TextPassword" /></td>
                </tr>
                <tr>
                    <td colspan="2">
                        <input type="submit" />
                    </td>
                </tr>
            </table>
        </form>
    </div>
</body>
</html>
